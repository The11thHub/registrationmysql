{
   "apiVersion": "v1",
   "kind": "Template",
   "metadata": {
      "name": "sample-application",
      "labels": {
         "app": "sample"
      }
   },
   "objects": [
      {
         "kind": "ImageStream",
         "apiVersion": "image.openshift.io/v1",
         "metadata": {
            "labels": {
               "app": "sample"
            },
            "name": "sample"
         },
         "spec": {
            "lookupPolicy": {
               "local": false
            }
         }
      },
      {
         "kind": "Build",
         "apiVersion": "build.openshift.io/v1",
         "metadata": {
            "labels": {
               "app": "sample"
            },
            "name": "sample-1",
            "namespace": "run"
         },
         "spec": {
            "nodeSelector": null,
            "output": {
               "to": {
                  "kind": "ImageStreamTag",
                  "name": "sample:latest"
               }
            },
            "postCommit": {},
            "resources": {},
            "serviceAccount": "builder",
            "source": {
               "git": {
                  "ref": "master",
                  "uri": "https://github.com/vicky1411/registrationmysql"
               },
               "type": "Git"
            },
            "strategy": {
               "sourceStrategy": {
                  "from": {
                     "kind": "ImageStreamTag",
                     "name": "jboss-eap64-openshift:1.4",
                     "namespace": "openshift"
                  }
               },
               "type": "Source"
            },
            "triggeredBy": [
               {
                  "message": "Build configuration change"
               }
            ]
         }
      },
      {
         "kind": "DeploymentConfig",
         "apiVersion": "apps.openshift.io/v1",
         "metadata": {
            "labels": {
               "app": "sample"
            },
            "name": "sample"
         },
         "spec": {
            "replicas": 2,
            "selector": {
               "deploymentconfig": "sample"
            },
            "strategy": {
               "activeDeadlineSeconds": 21600,
               "resources": {},
               "rollingParams": {
                  "intervalSeconds": 1,
                  "maxSurge": "25%",
                  "maxUnavailable": "25%",
                  "timeoutSeconds": 600,
                  "updatePeriodSeconds": 1
               },
               "type": "Rolling"
            },
            "template": {
               "metadata": {
                  "creationTimestamp": null,
                  "labels": {
                     "app": "sample",
                     "deploymentconfig": "sample"
                  }
               },
               "spec": {
                  "containers": [
                     {
                        "image": "sample:latest",
                        "imagePullPolicy": "Always",
                        "name": "sample",
                        "ports": [
                           {
                              "containerPort": 8080,
                              "protocol": "TCP"
                           },
                           {
                              "containerPort": 8443,
                              "protocol": "TCP"
                           }
                        ],
                        "resources": {},
                        "terminationMessagePath": "/dev/termination-log",
                        "terminationMessagePolicy": "File"
                     }
                  ],
                  "dnsPolicy": "ClusterFirst",
                  "restartPolicy": "Always",
                  "schedulerName": "default-scheduler",
                  "securityContext": {},
                  "terminationGracePeriodSeconds": 30
               }
            },
            "test": false,
            "triggers": [
               {
                  "imageChangeParams": {
                     "automatic": true,
                     "containerNames": [
                        "sample"
                     ],
                     "from": {
                        "kind": "ImageStreamTag",
                        "name": "sample:latest"
                     },
                     "lastTriggeredImage": null
                  },
                  "type": "ImageChange"
               },
               {
                  "type": "ConfigChange"
               }
            ]
         }
      },
      {
         "kind": "Service",
         "apiVersion": "v1",
         "metadata": {
            "labels": {
               "app": "sample"
            },
            "name": "sample"
         },
         "spec": {
            "ports": [
               {
                  "name": "8080-tcp",
                  "port": 8080,
                  "protocol": "TCP",
                  "targetPort": 8080
               }
            ],
            "selector": {
               "deploymentconfig": "sample"
            },
            "sessionAffinity": "None",
            "type": "ClusterIP"
         }
      }
   ]
}
